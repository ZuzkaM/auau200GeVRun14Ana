<?xml version="1.0" encoding="utf-8" ?> 
<job >
  <command>
    ln -s &baseDir;/&inputFile;
    
    ./runSimCutsMaker.sh &first; &last;

  </command>
  <stdout URL="file:&baseDir;/log/CutsOptimizationBGplots&first;.log" />
  <stderr URL="file:&baseDir;/err/CutsOptimizationBGplots&first;.err"/>
  <output fromScratch="bkg*.root" toURL="file:&baseDir;/" />

  <SandBox installer="ZIP">
    <Package name="LocalLibraries">
      <File>file:./runSimCutsMaker.sh</File>
      <File>file:./simCutsMaker/</File>
    </Package>
  </SandBox>

</job>

